<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cloud.system.mapper.UserMapper">

    <resultMap type="com.cloud.system.domain.User" id="user"></resultMap>

    <select id="getUser" parameterType="com.cloud.system.domain.User" resultMap="user">
        select id,name from `user` limit 1
    </select>

</mapper>